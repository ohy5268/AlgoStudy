# run time error 발생
for i in range(1, int(input())+1):
    N = int(input())
    ftr = tuple(map(int, input().split()))
    result = 0 
    while True:
        y = ftr.index(max(ftr))
        if y != 0:
            result += ftr[y]*len(ftr[:y]) - sum(ftr[:y])
        ftr = ftr[y+1:]
        if len(ftr) == 0:
            break
    print(f"#{i}", result)
